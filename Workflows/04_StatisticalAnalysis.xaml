    <Assign DisplayName="Assign minimum price from data table" sap:VirtualizedContainerService.HintSize="450,175" sap2010:WorkflowViewState.IdRef="Assign_1">
      <Assign.To>
        <OutArgument x:TypeArguments="x:Double">[dblMinPrice]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="x:Double">[dtRawProducts.AsEnumerable().Min(Function(row) CDbl(row("Price").ToString()))]</InArgument>
      </Assign.Value>
    </Assign>
    <Assign DisplayName="Assign maximum price from data table" sap:VirtualizedContainerService.HintSize="450,175" sap2010:WorkflowViewState.IdRef="Assign_2">
      <Assign.To>
        <OutArgument x:TypeArguments="x:Double">[dblMaxPrice]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="x:Double">[dtRawProducts.AsEnumerable().Max(Function(row) CDbl(row("Price").ToString()))]</InArgument>
      </Assign.Value>
    </Assign>
    <Assign DisplayName="Assign average price from data table" sap:VirtualizedContainerService.HintSize="450,175" sap2010:WorkflowViewState.IdRef="Assign_3">
      <Assign.To>
        <OutArgument x:TypeArguments="x:Double">[dblAvgPrice]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="x:Double">[dtRawProducts.AsEnumerable().Average(Function(row) CDbl(row("Price").ToString()))]</InArgument>
      </Assign.Value>
    </Assign>
    <Assign DisplayName="Assign total products from data table" sap:VirtualizedContainerService.HintSize="450,175" sap2010:WorkflowViewState.IdRef="Assign_4">
      <Assign.To>
        <OutArgument x:TypeArguments="x:Int32">[intTotalProducts]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="x:Int32">[dtRawProducts.RowCount]</InArgument>
      </Assign.Value>
    </Assign>
    <Assign DisplayName="Assign top 10 most expensive products to datatable" sap:VirtualizedContainerService.HintSize="450,175" sap2010:WorkflowViewState.IdRef="Assign_5">
      <Assign.To>
        <OutArgument x:TypeArguments="sd:DataTable">[GlobalVariablesNamespace.GlobalVariables.dtExpensiveProducts]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="sd:DataTable">[dtRawProducts.AsEnumerable().OrderByDescending(Function(row) CDbl(row("Price").ToString())).Take(10).CopyToDataTable()]</InArgument>
      </Assign.Value>
    </Assign>
    <Assign DisplayName="Assign top 10 most cheaper products" sap:VirtualizedContainerService.HintSize="450,175" sap2010:WorkflowViewState.IdRef="Assign_6">
      <Assign.To>
        <OutArgument x:TypeArguments="sd:DataTable">[GlobalVariablesNamespace.GlobalVariables.dtCheaperProducts]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="sd:DataTable">[dtRawProducts.AsEnumerable().OrderBy(Function(row) CDbl(row("Price").ToString())).Take(10).CopyToDataTable()]</InArgument>
      </Assign.Value>
    </Assign>
